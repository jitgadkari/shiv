{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "shiv-frontend/build",
        "use": "@now/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/shiv-frontend/build/$1"
      }
    ]
  }